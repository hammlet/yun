<view><cmd-nav-bar vue-id="1" back title="用户注册" background-color="#048ABF" font-color="#FFFFFF" bind:__l="__l"></cmd-nav-bar><cmd-page-body vue-id="2" type="top" bind:__l="__l" vue-slots="{{['default']}}"><view class="register"><view class="register-title">{{status?'手机快捷注册':'账号密码注册'}}</view><view class="register-explain">{{status?'用户可通过手机验证码直接注册':'使用账号密码注册后请绑定手机号'}}</view><cmd-transition vue-id="{{('3')+','+('2')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><block wx:if="{{status}}"><view><view class="register-phone"><cmd-input bind:input="__e" vue-id="{{('4')+','+('3')}}" type="number" focus maxlength="11" placeholder="请输入手机号" value="{{mobile.phone}}" data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['mobile']]]]]}}" bind:__l="__l"></cmd-input><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="register-phone-getcode" bindtap="__e">{{!safety.state&&'获取验证码'||safety.time+' s'}}</view></view><view class="register-code"><cmd-input bind:input="__e" vue-id="{{('5')+','+('3')}}" type="number" maxlength="6" placeholder="请输入短信验证码" value="{{mobile.code}}" data-event-opts="{{[['^input',[['__set_model',['$0','code','$event',[]],['mobile']]]]]}}" bind:__l="__l"></cmd-input></view><button class="{{['btn-register',registerMobile?'btn-register-active':'']}}" disabled="{{!registerMobile}}" hover-class="btn-register-hover" data-event-opts="{{[['tap',[['fnRegister',['$event']]]]]}}" bindtap="__e">注册</button></view></block></cmd-transition></view></cmd-page-body></view>